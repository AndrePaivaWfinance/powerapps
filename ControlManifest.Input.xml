<?xml version="1.0" encoding="utf-8" ?>
<manifest
  xmlns="http://schemas.microsoft.com/applocalization/2018/manifest"
  xmlns:control="http://schemas.microsoft.com/xrm/2019/NEWControl"
  version="1.0.0">

  <control:control
      namespace="Finance"
      constructor="DashboardFinanceiro"
      version="1.0.0"
      display-name-key="DashboardFinanceiro"
      description-key="Dashboard financeiro customizado">

    <control:resources>
      <control:code path="index.ts" order="1" />
      <control:css path="style.css" order="1" />
    </control:resources>

    <control:property name="Saldo" display-name-key="Saldo" description-key="Saldo atual"
      of-type="Decimal" usage="bound" />

    <control:property name="Receita" display-name-key="Receita" description-key="Receita atual"
      of-type="Decimal" usage="bound" />

    <control:property name="Despesa" display-name-key="Despesa" description-key="Despesa atual"
      of-type="Decimal" usage="bound" />

  </control:control>

</manifest>
